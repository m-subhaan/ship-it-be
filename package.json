    "dependencies": {
        "@sendgrid/mail": "8.1.2",
        "async": "3.2.5",
        "aws-sdk": "2.1620.0",
        "bcrypt": "5.1.1",
        "commander": "12.0.0",
        "cors": "2.8.5",
        "dotenv": "16.4.5",
        "express": "4.19.2",
        "jsonwebtoken": "9.0.2",
        "luxon": "3.4.4",
        "pg": "8.11.5",
        "tsyringe": "4.8.0",
        "typeorm": "0.3.20",
        "uuid": "9.0.1",
        "winston": "3.13.0",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.2.1",
        "@types/async": "3.2.24",
        "@types/bcrypt": "5.0.2",
        "@types/cors": "2.8.17",
        "@types/express": "4.17.21",
        "@types/jsonwebtoken": "9.0.6",
        "@types/luxon": "3.4.2",
        "@types/node": "20.12.2",
        "@types/uuid": "9.0.8",
        "@typescript-eslint/eslint-plugin": "7.9.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "husky": "9.0.11",
        "lint-staged": "15.2.2",
        "nodemon": "3.1.0",
        "prettier": "3.2.5",
        "ts-node": "10.9.2",
        "tsc-alias": "1.8.8",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.4.3"
    },
    "packageManager": "pnpm@9.1.1"
