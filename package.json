{
    "name": "ship-it-be",
    "version": "1.6.0",
    "description": "Ship It Api",
    "main": "build/bin/www.js",
    "scripts": {
        "prepare": "husky",
        "dev": "node -r ts-node/register --watch --watch-preserve-output bin/www.ts",
        "test": "node --test",
        "build": "tsc && tsc-alias"
